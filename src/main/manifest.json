{
  "name": "Tab Suspender",
  "author": "Michalewicz Piotr",
  "version": "3.7.4",
  "description": "Lightweight and performant open source extension that reduce Firefox's memory usage.",
  "manifest_version": 2,
  "icons": {
    "16": "fox-16px.png",
    "48": "fox-48px.png",
    "96": "fox-96px.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "options_ui": {
    "page": "options.html"
  },
  "permissions": [
    "<all_urls>",
    "tabs",
    "storage",
    "menus",
    "contextMenus"
  ],
  "applications": {
    "gecko": {
      "id": "tabsuspender@michalewiczpiotr",
      "strict_min_version": "59.0"
    }
  }
}
